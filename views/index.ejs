<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="/socket.io/socket.io.js"></script>
  <link rel="stylesheet" href="style.css">
  <title>Webchat</title>
</head>
<body>
  <form id="chat">
    <input type="text" name="nickname" placeholder="Digite seu usuario">
    <div class="messages"></div>
    <input type="text" data-testid="message-box" name="message-box" placeholder="Digite sua mensagem aqui"/>
    <button type="submit" data-testid="send-button" name="send-button">Enviar</button>
  </form>
  <script src="./script.js"></script>
</body>
</html>