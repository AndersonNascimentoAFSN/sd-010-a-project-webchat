<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="/socket.io/socket.io.js"></script>
  <link rel="stylesheet" href="style.css">
  <title>Webchat</title>
</head>
<body>
 <div id="chat">
    <input id="username" type="text" name="nickname" placeholder="Digite seu usuario">
    <div class="messages"></div>
    <input type="text" id="message-teste" data-testid="message-box" name="message-box" placeholder="Digite sua mensagem aqui"/>
    <button id="button-teste" data-testid="send-button" name="send-button">Enviar</button>
  </div>
  <script src="./script.js"></script>
</body>
</html>